<p id="notice"><%= notice %></p>

  <section class="listings">
  <div class="wrapper">
    <ul class="properties_list">
    
        <% @hotels.each do |hotel| %>
        <tr>
         
        <td>
          
        </td>
        <li>
        <a href="#">
          <%hotel.picture.each do |image|%>   
          <%=  image_tag image.url %>  
          <% end%>
        </a>
        <span class="price">$2500</span>
        <div class="property_details">
          <h1>
          <td><%= hotel.name %></td>
          </h1>
          <h2><td><%= hotel.description %></td> <span class="property_size">(288ftsq)</span></h2>
          <td><%= hotel.user.name %></td>

          <td><%= link_to 'Show', hotel %></td>
          

        </div>
      </li>
      
        
      </tr>
    <% end %>

</ul>
</div>
</section>
<br>

<%= link_to 'New Hotel', new_hotel_path %>

